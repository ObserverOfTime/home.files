[Appearance]
AntiAliasFonts=true
BoldIntense=false
ColorScheme=Gruvbox8
Font=Hack,16,-1,0,50,0,0,0,0,0
LineSpacing=1
UseFontLineChararacters=true

[Cursor Options]
CursorShape=2

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/bin/nvim +term
Directory=~
Environment=TERM=konsole-256color,COLORTERM=truecolor
Name=NvimTerm
Parent=FALLBACK/
TerminalCenter=true
TerminalColumns=100
TerminalMargin=2
TerminalRows=35

[Scrolling]
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=true

# vim:ft=cfg:
